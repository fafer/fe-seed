{
  "name": "fe-seed",
  "version": "1.0.0",
  "description": "fe develop seed project",
  "main": "index.js",
  "bin": {
    "deploy": "./build/script/ftp/index.js"
  },
  "scripts": {
    "prestart": "npm run build:dll",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.dev.js --progress",
    "premock": "npm run prestart",
    "mock": "cross-env NODE_ENV=development MOCK_DATA=mock webpack-dev-server --config ./build/webpack.config.dev.js --progress",
    "prebuild": "npm run prestart",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.js --progress --profile --colors",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ftp": "npm run build && npm run ftp",
    "build:dll": "cross-env NODE_ENV=production webpack --config ./build/webpack.dll.js --progress --profile --colors",
    "add": "node ./build/script/add",
    "eslint": "eslint . --ext .js,.jsx --fix",
    "tslint": "tslint src/**/*.{ts,tsx} --fix",
    "stylelint": "stylelint  src/**/*.{css,scss} --fix",
    "lint": "npm run eslint && npm run tslint && npm run stylelint",
    "deploy": "node ./build/script/ftp/index",
    "ftp": "ftp-cli",
    "test": "jest sum.test --coverage",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/ ./package-lock.json",
    "codecov": "cat ./coverage/lcov.info | ./node_modules/.bin/codecov"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{ts,tsx}": [
      "npm run tslint",
      "git add"
    ],
    "*.{css,scss}": [
      "npm run stylelint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fafer/fe-seed.git"
  },
  "homepage": "https://github.com/fafer/fe-seed#readme",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "fafer",
  "license": "MIT",
